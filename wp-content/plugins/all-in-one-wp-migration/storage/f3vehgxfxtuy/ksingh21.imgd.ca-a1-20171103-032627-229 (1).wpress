		if ( !empty( $w3tc_start_microtime ) ) {
			$ms10 = (int)( ( microtime( true ) - $w3tc_start_microtime ) * 100 );
			$storage->counter_add( 'pagecache_requests_time_10ms', $ms10 );
		}
	}



	/**
